$('.js-form').html('<%= escape_javascript render('form') %>');

ReactDOM.render(
  React.createElement(Rating, {rating: <%= @event.rating.to_i %>, top: <%= Event::TOP %>}),
  document.getElementById('js-rating')
);

<% unless @event.new_record? %>
  ReactDOM.render(
    React.createElement(Photos, {photos: <%= raw @assets.to_json %>, url: "<%= assets_path(event_id: @event.id) %>"}),
    document.getElementById('js-assets')
  );
<% end %>
