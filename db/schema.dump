CREATE TABLE actividad
(
  id serial primary key,
  nombre varchar
)
WITH (
  OIDS=FALSE
);
ALTER TABLE actividad OWNER TO pruebas;

CREATE TABLE usuario
(
  id serial primary key,
  email varchar,
  clave varchar,
  can_download boolean,
  can_edit_actividad boolean,
  can_edit_efemeride boolean,
  can_see_leyenda boolean,
  can_see_resumen boolean
)
WITH (
  OIDS=FALSE
);
ALTER TABLE usuario OWNER TO pruebas;

